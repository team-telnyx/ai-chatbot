{
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "../app",
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "ES2022",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
